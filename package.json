{
  "name": "typescript-rest-api",
  "version": "1.0.0",
  "description": "API RESt desenvolvida com TypeScript, Node, ES6 e MongoDB",
  "main": "server.ts",
  "scripts": {
    "build": "./node_modules/.bin/tsc ./server/server.ts && gulp",
    "start-production": "node ./build/server.js",
    "start-dev": "NODE_ENV=development nodemon ./node_modules/.bin/ts-node ./server/server.ts"
  },
  "keywords": [
    "API",
    "TypeScript",
    "Node",
    "Mongo"
  ],
  "author": "Natan Severo",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.17.2",
    "express": "^4.15.3",
    "http-status": "^1.0.1",
    "mongoose": "^4.11.3",
    "morgan": "^1.8.2"
  },
  "devDependencies": {
    "@types/body-parser": "^1.16.4",
    "@types/express": "^4.0.36",
    "@types/http-status": "^0.2.29",
    "@types/mongoose": "^4.7.19",
    "@types/morgan": "^1.7.32",
    "@types/node": "^8.0.14",
    "gulp": "^3.9.1",
    "gulp-clean": "^0.3.2",
    "ts-node": "^3.2.1",
    "typescript": "^2.4.2"
  }
}
